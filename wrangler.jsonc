{
  "$schema": "https://unpkg.com/wrangler@latest/config-schema.json",
  "name": "log-viewer",
  "compatibility_date": "2025-09-03",
  "main": "./dist/worker/index.js",
  "upload_source_maps": true,
  "assets": {
    "directory": "./dist/client",
    "binding": "ASSETS",
    "not_found_handling": "single-page-application"
  },
  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
