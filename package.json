    "react-error-boundary": "^4.1.2",
    "react-router": "^7.1.3",
    "react-scripts": "5.0.1",
    "swr": "^2.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@cloudflare/workers-types": "^4.20250121.0",
    "@types/react": "^19.0.7",
