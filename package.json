{
  "name": "log-viewer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:worker": "wrangler pages functions build --outdir=./dist/worker/",
    "build:client": "vite build",
    "build": "pnpm run build:worker && pnpm run build:client",
    "lint": "biome check && tsc --noEmit",
    "lint:fix": "biome check --write && tsc --noEmit",
    "preview": "pnpm run build && vite preview",
    "deploy": "pnpm run build && wrangler deploy"
  },
  "dependencies": {
    "@widgetbot/message-renderer": "^2.3.2",
    "discord-api-types": "^0.38.22",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-router": "^7.8.2",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.2",
    "@cloudflare/vite-plugin": "^1.12.2",
    "@cloudflare/workers-types": "^4.20250903.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "vite-plugin-svgr": "^4.5.0",
    "wrangler": "^4.33.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "core-js-pure",
      "es5-ext",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
